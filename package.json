{
  "name": "blueprint-templates-cli",
  "version": "0.1.2",
  "description": "CLI for vscode blueprint-templates extension",
  "main": "./out/src/cli.js",
  "author": "Anton Pavlov",
  "license": "MIT",
  "bin": {
    "create-component": "./out/src/cli.js"
  },
  "scripts": {
    "lint": "node ./node_modules/tslint/bin/tslint \"src/**/*.ts\"",
    "prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "create-component": "chmod +x ./out/src/cli.js && ./out/src/cli.js"
  },
  "devDependencies": {
    "@types/node": "^10.12.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/inquirer": "^0.0.43",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.12",
    "inquirer": "^6.2.0",
    "inquirer-path": "^1.0.0-beta5"
  }
}
